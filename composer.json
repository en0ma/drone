{
    "name": "en0ma/drone",
    "description": "A dynamodb migration tool",
    "type": "library",
    "keywords": [
        "dynamodb",
        "migration",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Enehizena",
            "email": "lawstands@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^3.2",
        "aws/aws-sdk-php": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "en0ma\\Drone\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "en0ma\\Drone\\Tests\\": "tests/"
        }
    },
    "bin": ["drone"]
}
